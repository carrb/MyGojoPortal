<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, user-scalable=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta http-equiv="cleartype" content="on">
    
    <link type="text/css" rel="stylesheet" href="~/Content/CSS/bootstrap.css">
    <style>body{padding-top:60px;padding-bottom:40px;}.sidebar-nav{padding:9px 0;}h1.homepage { font: 800 64px/64px "open sans";margin: 0 0 32px 0;}</style>
    <link type="text/css" rel="stylesheet" href="~/Content/CSS/bootstrap-responsive.css"/>
    <link type="text/css" rel="stylesheet" href="~/Content/CSS/font-awesome.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/CSS/jquery.jgrowl.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/CSS/prettify.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/CSS/redactor.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/CSS/overrides.css" />
    
   @* @Styles.Render("~/bundles/less");*@

    <link type="text/css" rel="stylesheet" href="//fonts.googleapis.com/css?family=Lato|Cantarell&v2" />
    <link type="text/css" rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" />

    @* Need to manual instead of Bundles and check problems?
    @Styles.Render("~/Content/themes/base/css", "~/Content/css", "~/Content/CSS")
     *@

    <!--[if (lt IE 9) & (!IEMobile)]>
        <script src="~/Scripts/selectivizr-min.js"></script>
        <script src="~/Scripts/imgSizer.js"></script>
        <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <script type="text/javascript" src="~/Scripts/ba-debug.min.js"></script>
    <script type="text/javascript" src="~/Scripts/modernizr.js"></script>
    <script type="text/javascript" src="~/Scripts/respond.min.js"></script>
   @* @Scripts.Render("~/bundles/modernizr")*@
</head>
<body>
    <!--[if lt IE 7]>
        <link href="~/Content/CSS/bootstrap.ie6.css" rel="stylesheet" />
  	    <p class="chromeframe">Your browser is <em>ancient!</em> <a href="http://browsehappy.com/">Upgrade to a different browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to experience this site.</p>
    <![endif]-->
    
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container-fluid">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="brand" href="~/">MyGOJO</a>
                <!-- Everything you want hidden at 940px or less, place within here -->
                <div class="nav-collapse">
                    @Html.Partial("_HeaderBarQuickLinks")
                    @Html.Partial("_DropDownNav")
                    
                    <p class="navbar-text pull-right">[ <strong>@User.Identity.Name</strong> ]</p>
                </div><!--/.nav-collapse -->
            </div>
        </div>
    </div>

    <div class="container-fluid">
		@RenderBody() 
	</div><!--/.fluid-container-->
        
    <div class="container-fluid">
        @Html.Partial("_Footer")    
    </div><!--/.fluid-container-->
    
    
    <script type="text/javascript" src="~/Scripts/jquery-1.8b1.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-ui-1.8.20.min.js"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
    @*<script type="text/javascript" src="~/Scripts/ember-0.9.8.1.js"></script>*@
    <script type="text/javascript" src="~/Scripts/jquery.jgrowl.min.js"></script>
    @* See: http://www.netmagazine.com/tutorials/make-stylish-preloader-svg *@
    <script type="text/javascript" src="~/Scripts/jquery.queryloader2.js"></script>  
    <script type="text/javascript" src="~/Scripts/prettify.js"></script>
    <script type="text/javascript" src="~/Scripts/redactor.min.js"></script>
    <script type="text/javascript" src="~/Scripts/plugins.js"></script>
    <script type="text/javascript" src="~/Scripts/app.js"></script>
    

@* Need to manual instead of Bundles and check problems?
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/extra")
     *@
    @RenderSection("Scripts", required: false)
    
@*
    <!-- A little ie7- magic -->
    <script type="text/javascript"> $(function () { if ($.browser.msie && parseInt($.browser.version, 10) === 6) { $('.row div[class^="span"]:last-child').addClass("last-child"); $('[class="span"]').addClass("margin-left-20"); $(':button[class="btn"], :reset[class="btn"], :submit[class="btn"], input[type="button"]').addClass("button-reset"); $(":checkbox").addClass("input-checkbox"); $('[class^="icon-"], [class=" icon-"]').addClass("icon-sprite"); $(".pagination li:first-child a").addClass("pagination-first-child") } }) </script>
*@
</body>
</html>
