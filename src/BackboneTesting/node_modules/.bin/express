#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/c:\Projects\GitHub\MyGojoPortal\src\BackboneTesting\node_modules\express\bin\express" "$@"
  ret=$?
else 
  node  "$basedir/c:\Projects\GitHub\MyGojoPortal\src\BackboneTesting\node_modules\express\bin\express" "$@"
  ret=$?
fi
exit $ret
